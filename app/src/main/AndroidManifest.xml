<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.telechaplaincy">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".chaplain_sign_activities.ChaplainSignUp"
            android:parentActivityName=".chaplain_sign_activities.ChaplainLogIn"></activity>
        <activity android:name=".ChaplainMainActivity" />
        <activity android:name=".chaplain_sign_activities.ChaplainLogIn" />
        <activity
            android:name=".patient_sign_activities.ForgotPassword"
            android:parentActivityName=".patient_sign_activities.LoginPage" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".patient_sign_activities.SignUpActivity"
            android:parentActivityName=".patient_sign_activities.LoginPage" />
        <activity android:name=".patient_sign_activities.LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>